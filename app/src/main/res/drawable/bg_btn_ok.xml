<?xml version="1.0" encoding="utf-8"?>

<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- rectangle表示为矩形 -->

    <!-- 填充的颜色 -->
    <solid android:color="#1AFFFFFF" />

    <!-- 边框的粗细和颜色 -->
    <stroke
        android:color="#1AFFFFFF"/>
    <!-- android:radius 圆角的半径 -->
    <corners
        android:radius="38pt"/>
</shape>
